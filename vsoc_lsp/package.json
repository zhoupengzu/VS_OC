{
  "name": "vsoc_lsp",
  "displayName": "VSOC LSP",
  "description": "VSCode extension for Objective-C language support using LSP",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:objective-c"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "objective-c",
        "aliases": [
          "Objective-C",
          "objc"
        ],
        "extensions": [
          ".m",
          ".h"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "typescript": "^4.7.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
